{
  "name": "@nordicsemiconductor/rsrp-bar",
  "version": "0.0.0-development",
  "description": "React component to render an RSRP bar or a failover icon in case the reported value is not valid",
  "scripts": {
    "lint": "eslint src/**",
    "test": "npx tsx --test ./src/*.spec.ts",
    "prepare": "husky",
    "start": "vite"
  },
  "type": "module",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "node": "./dist/index.js"
    }
  },
  "homepage": "https://nordicsemiconductor.github.io/asset-tracker-cloud-docs/",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/rsrp-bar-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/rsrp-bar-js/issues"
  },
  "devDependencies": {
    "@bifravst/eslint-config-typescript": "6.0.17",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "19.1.0",
    "@swc/register": "0.1.10",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint-plugin-react": "7.34.1",
    "husky": "9.0.11",
    "source-map-loader": "5.0.0",
    "ts-loader": "9.5.1",
    "vite": "5.2.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "files": [
    "README.md",
    "dist",
    "package-lock.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "prettier": "@bifravst/prettier-config"
}
